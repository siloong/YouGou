
/* swiper */
.index_swiper swiper{
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}


/* navigation */
.index_navigation {
  display: flex;
}
.index_navigation navigator {
  flex: 1;
  padding: 10rpx;
}
.index_navigation image {
  width: 100%;
}


/* floor */
/* .index_floor{} */
.floor_title {
  padding: 40rpx 10rpx 0 0;
}
.floor_title image {
  width: 100%;
}

.floor_content {
  overflow: hidden;
}
.floor_content navigator {
  float: left;
  width: calc(100vw / 3);
}
.floor_content navigator:nth-last-child(-n+4) {
  height: calc(100% / 2);
  /* border: solid 1px red; */
  height: calc(33.33vw * 386 /  232 /2);
  padding-left: 10rpx;
}
.floor_content navigator:nth-child(2),
.floor_content navigator:nth-child(3) {
  padding-bottom: 10rpx;
}
.floor_content navigator image {
  width: 100%;
  height: 100%;
}